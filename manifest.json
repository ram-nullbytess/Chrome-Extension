{
    "name": "AutoWeather",
    "version": "1.0",
    "manifest_version": 2,
    "description": "Check the weather ",
    "icons": {
        "128": "images/icon-128.png"
    },
    "permissions": ["geolocation"],
    "browser_action": {
        "default_icon": {
            "38": "images/icon-128.png"
        },
        "default_title": "AutoWeather",
        "default_popup": "popup.html"
    }
}
